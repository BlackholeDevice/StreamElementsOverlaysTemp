{
  "field<%- id %>enabled": {
    "type": "checkbox",
    "label": "Enabled",
    "value": true,
    "group": "<%- item %>"
  },
  "field<%- id %>label": {
    "type": "string",
    "label": "Field label",
    "value": "<%- item %>",
    "group": "<%- item %>"
  },
  "field<%- id %>icon": {
    "type": "image-input",
    "label": "Field icon",
    "value": "",
    "group": "<%- item %>"
  },
  "field<%- id %>command": {
    "type": "string",
    "label": "Field command",
    "value": "!command",
    "group": "<%- item %>"
  },
  "field<%- id %>type": {
    "type": "dropdown",
    "label": "Field type",
    "options": {
      "accumulate": "Accumulator",
      "counter": "Counter"
    },
    "group": "<%- item %>"
  },
  "field<%- id %>permission": {
    "type": "dropdown",
    "label": "Command permission",
    "value": "broadcaster",
    "options": {
      "broadcaster": "Broadcaster",
      "moderator": "Moderator",
      "vip": "VIP",
      "subscriber": "Subscriber",
      "everyone": "Everyone"
    },
    "group": "<%- item %>"
  }
}